el-before($url, $display, $w, $h, $margin)
    &:before
        content: ''
        display:$display
        width:$w
        height:$h
        background:url($url)no-repeat
        background-size:100%
        margin:$margin
el-before2($url, $display, $w, $h, $margin)
    &:before
        content: ''
        display:$display
        width:$w
        height:$h
        background:url($url)no-repeat
        background-size:100% 100%
        margin:$margin
        vertical-align:bottom
        margin-right:3px
.serveRoot
    padding-top:45px
    .mint-popup
        width:100%
        .carNode
            width:100%
            height:45px
            line-height:45px
            text-align:center
            border-bottom:1px solid #eee
    .serveType
        background-color:#fff
        display:flex
        border-bottom:1px solid #ddd
        a
            height:59px
            padding-top:6px
            flex:1
            text-align:center
            font-size:15px
            &:first-child
                el-before('../../assets/images/icon_serve_1.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(2)
                el-before('../../assets/images/icon_serve_2.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(3)
                el-before('../../assets/images/icon_serve_3.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(4)
                el-before('../../assets/images/icon_serve_4.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(5)
                el-before('../../assets/images/icon_serve_5.png', block, 30px, 30px, 0 auto 5px auto)
        .router-link-active
            color:#c7a76f
            &:first-child
                el-before('../../assets/images/icon_serve_1_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(2)
                el-before('../../assets/images/icon_serve_2_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(3)
                el-before('../../assets/images/icon_serve_3_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(4)
                el-before('../../assets/images/icon_serve_4_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(5)
                el-before('../../assets/images/icon_serve_5_sel.png', block, 30px, 30px, 0 auto 5px auto)
    .serveCommon
        .serveCommonWrap
            margin-top:10px
            .listWrap
                .list
                    background-color:#fff
                    height:120px
                    padding-left:140px
                    position:relative
                    border-bottom:1px solid #ddd
                    img
                        position:absolute
                        left:10px
                        top:10px
                        width:120px
                        height:90px
                    .info
                        position:relative
                        padding-top:10px
                        .tt
                            height:36px
                            color:#333
                            font-size:18px
                            letter-spacing:1px
                            width:83%
                        .desc
                            margin-top:3px
                            color:#999
                            height:38px
                            line-height:18px
                            overflow:hidden
                        .iconGroup
                            margin-top:5px
                            font-size:15px
                            color:#999
                            position:relative
                            .door
                                el-before2('../../assets/images/icon_serve_6.png', inline-block,16px, 16px, 0)
                            .store
                                el-before2('../../assets/images/icon_serve_8.png', inline-block,16px, 16px, 0)
                            .link
                                float:right
                                position:absolute
                                left:130px
                                el-before2('../../assets/images/icon_serve_7.png', inline-block,16px, 16px, 0)
                    .cart
                        width:22px
                        height:22px
                        background:url(../../assets/images/icon_serve_9.png)no-repeat
                        background-size:22px 22px
                        position:absolute
                        right:10px
                        top:10px

@media screen and (max-width: 374px)
    .serveRoot
        .serveCommon
            .serveCommonWrap
                .listWrap
                    .list
                        padding-left:110px
                        height:100px
                        position:relative
                        img
                            position:absolute
                            left:5px
                            top:5px
                            width:100px
                            height:75px
                        .info
                            padding-top:2px
                            .tt
                                height:34px
                                color:#333
                                font-size:16px
                                letter-spacing:0px
                                width:75%
                            .desc
                                margin-top:2px
                                color:#999
                                height:36px
                                font-size:14px
                                padding-right:3px
                                line-height:18px
                                overflow:hidden
                            .iconGroup
                                margin-top:3px
                                font-size:14px
                                color:#999
                                position:relative
                                .door
                                    el-before2('../../assets/images/icon_serve_6.png', inline-block,16px, 16px, 0)
                                .store
                                    el-before2('../../assets/images/icon_serve_8.png', inline-block,16px, 16px, 0)
                                .link
                                    float:right
                                    position:absolute
                                    left:120px
                                    el-before2('../../assets/images/icon_serve_7.png', inline-block,16px, 16px, 0)
