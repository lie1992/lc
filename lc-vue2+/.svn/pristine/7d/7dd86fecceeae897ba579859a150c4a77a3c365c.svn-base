        component: cityView
    }, {
        path: '/message',
        component: message
    }, {
        path: '/message/:id',
        component: messageDetail
    }, {
        path: '/register',
        component: register
    }, {
        path: '/login',
        component: login
    }, {
        path: '/findPassword',
        component: findPassword
    }, {
        path: '/user',
        component: user,
