            path: '',
            component: userIndex
        }, {
            path: 'index',
            component: userIndex
        }, {
            path: 'carList',
            component: carList
        }, {
            path: 'report',
            component: report
        }, {
            path: 'address',
            component: address
        }, {
            path: 'issure',
            component: issure
        }, {
            path: 'share',
            component: share
        }, {
            path: 'idea',
            component: idea
        }, {
            path: 'info',
            component: info
        }, {
            path: 'discount',
            component: discount
        }, {
            path: 'balance',
            component: balance
        }, {
            path: 'setting',
            component: setting
        }]
    }
]

export default {
    routes
