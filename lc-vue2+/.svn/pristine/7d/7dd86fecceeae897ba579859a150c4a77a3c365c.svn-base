import info from './views/user/information.vue'
import discount from './views/user/discount.vue'
import balance from './views/user/balance.vue'
import setting from './views/user/setting.vue'

const routes = [
    {
        path: '/index',
        component: index
    }, {
        path: '/',
        component: index
    }, {
        path: '/cart',
        component: cart
    }, {
        path: '/order',
        component: order
    }, {
        path: '/user',
        component: user
