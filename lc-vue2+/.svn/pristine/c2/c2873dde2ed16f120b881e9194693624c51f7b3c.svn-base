el-before($url, $display, $w, $h, $margin)
    &:before
        content: ''
        display:$display
        width:$w
        height:$h
        background:url($url)no-repeat
        background-size:100%
        margin:$margin
el-before2($url, $display, $w, $h, $margin)
    &:before
        content: ''
        display:$display
        width:$w
        height:$h
        background:url($url)no-repeat
        background-size:100% 100%
        margin:$margin
        vertical-align:bottom
        margin-right:3px
.serveRoot
    padding-top:45px
    .mint-popup
        width:100%
        .carNode
            width:100%
            height:45px
            line-height:45px
            text-align:center
            border-bottom:1px solid #eee
    .serveType
        background-color:#fff
        border-bottom:1px solid #ddd
        overflow:hidden
        a
            height:59px
            padding-top:6px
            display:block
            float:left
            width:20%
            text-align:center
            font-size:15px
            color:#353535
            &:first-child
                el-before('../../assets/images/icon_serve_1.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(2)
                el-before('../../assets/images/icon_serve_2.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(3)
                el-before('../../assets/images/icon_serve_3.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(4)
                el-before('../../assets/images/icon_serve_4.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(5)
                el-before('../../assets/images/icon_serve_5.png', block, 30px, 30px, 0 auto 5px auto)
        .sel
            color:#e4ae0e
            &:first-child
                el-before('../../assets/images/icon_serve_1_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(2)
                el-before('../../assets/images/icon_serve_2_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(3)
                el-before('../../assets/images/icon_serve_3_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(4)
                el-before('../../assets/images/icon_serve_4_sel.png', block, 30px, 30px, 0 auto 5px auto)
            &:nth-child(5)
                el-before('../../assets/images/icon_serve_5_sel.png', block, 30px, 30px, 0 auto 5px auto)
    .serveCommon
        .serveCommonWrap
            margin-top:10px
            .listWrap
                .list
                    background-color:#fff
                    height:120px
                    padding-left:140px
                    position:relative
                    border-bottom:1px solid #ddd
                    img
                        position:absolute
                        left:10px
                        top:10px
                        width:120px
                        height:90px
                    .info
                        position:relative
                        padding-top:10px
                        .tt
                            height:36px
                            color:#333
                            font-size:18px
                            letter-spacing:1px
                            width:83%
                            overflow:hidden
                        .desc
                            margin-top:3px
                            color:#999
                            height:38px
                            line-height:18px
                            overflow:hidden
                        .iconGroup
                            margin-top:5px
                            font-size:15px
                            color:#999
                            position:relative
                            .door
                                el-before2('../../assets/images/icon_serve_6.png', inline-block,16px, 16px, 0)
                            .store
                                el-before2('../../assets/images/icon_serve_8.png', inline-block,16px, 16px, 0)
                            .link
                                float:right
                                position:absolute
                                left:130px
                                el-before2('../../assets/images/icon_serve_7.png', inline-block,16px, 16px, 0)
                    .cart
                        width:22px
                        height:22px
                        background:url(../../assets/images/icon_serve_9.png)no-repeat
                        background-size:22px 22px
                        position:absolute
                        right:10px
                        top:10px
    .tip
        text-align:center
        font-size:13px
        color:#323232
    .menuList
        /*margin-left:45px*/
        padding-left:45px
        margin-bottom:10px
        background-color:#fff
        border-bottom:1px solid #ddd
        border-top:1px solid #ddd
        margin-top:10px
        p
            position:relative
            height:45px
            border-bottom:1px solid #ddd
            line-height:45px
            &:before
                position:absolute
                right:10px
                font-size:14px
                color:#323232
            &:last-child
                border:none
            a
                color:#323232
                position:relative
                display:block
                &:before
                    position:absolute
                    content:''
                    display:inline-block
                    width:20px
                    height:20px
                    background-repeat:no-repeat
                    left:-45px
                    vertical-align:middle
                    background-size:100% 100%
                    margin:13px
            .phone
                color:#323232
                position:relative
                display:block
            .phoneShow
                display:block
                &:before
                    position:absolute
                    content:''
                    display:inline-block
                    width:20px
                    height:20px
                    background-repeat:no-repeat
                    left:-45px
                    vertical-align:middle
                    background-size:100% 100%
                    margin:13px
                    background-image:url(../../assets/images/icon_phone.png)
                .phoneNum
                    color:#999
                    float:right
                    margin-right:30px
                    font-style:normal
.searchWrap
    background:#fff
    border-bottom:1px solid #ddd
    position:relative
    input
        background:#eee
        display:block
        width:95%
        margin:5px auto
        height:30px
        line-height:30px
        border-radius:7px
        text-align:center
        color:#999
    .searchMask
        position:absolute
        width:100%
        height:46px
        top:-5px
        left:0
        line-height:46px
        text-align:center


@media screen and (max-width: 374px)
    .serveRoot
        .serveCommon
            .serveCommonWrap
                .listWrap
                    .list
                        padding-left:110px
                        height:100px
                        position:relative
                        img
                            position:absolute
                            left:5px
                            top:5px
                            width:100px
                            height:75px
                        .info
                            padding-top:2px
                            .tt
                                padding-top:1px
                                height:32px
                                color:#333
                                font-size:16px
                                letter-spacing:0px
                                width:75%
                                overflow:hidden
                            .desc
                                margin-top:2px
                                color:#999
                                height:36px
                                font-size:14px
                                padding-right:3px
                                line-height:18px
                                overflow:hidden
                            .iconGroup
                                margin-top:3px
                                font-size:14px
                                color:#999
                                position:relative
                                .door
                                    el-before2('../../assets/images/icon_serve_6.png', inline-block,16px, 16px, 0)
                                .store
                                    el-before2('../../assets/images/icon_serve_8.png', inline-block,16px, 16px, 0)
                                .link
                                    float:right
                                    position:absolute
                                    left:120px
                                    el-before2('../../assets/images/icon_serve_7.png', inline-block,16px, 16px, 0)
