                closeEmit: 'back2'
            },
            close: 'close'
        }
    },
    components: {
        'header-common': headerCommon
    },
    created () {
    },
    methods: {
        back2Rel () {
            var push = this.$route.query.push
            if (push) {
                this.$router.go(-1)
            } else {
                this.$router.go(-2)
            }
        }
    },
    beforeCreate () {
        this.$store.commit('hideFoot')
    }
}
</script>

